    ["Forest"] = {Color3.fromRGB(76, 214, 115), Color3.fromRGB(68, 173, 142), Color3.fromRGB(59, 130, 165)},
    ["Amethyst"] = {Color3.fromRGB(153, 97, 217), Color3.fromRGB(97, 97, 217), Color3.fromRGB(97, 162, 217)},

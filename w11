    ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 65, 80)), 
    ColorSequenceKeypoint.new(0.50, Color3.fromRGB(117, 76, 236)), 
    ColorSequenceKeypoint.new(1.00, Color3.fromRGB(66, 153, 225))
}}

